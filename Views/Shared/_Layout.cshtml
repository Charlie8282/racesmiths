<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] -  Racesmiths</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/css/app-dark.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" />


    @*<link src="~/favicon.ico" />
        <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.22/css/jquery.dataTables.css">
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Raleway:200,500,700,800" />
        <link rel="stylesheet" href="sweetalert2.min.css">
        <link href="~/css/bootstrap-dark.css" rel="stylesheet" />
        <link href="~/css/app-dark.min.css" rel="stylesheet" />
        <link href="~/css/app-dark.css" rel="stylesheet" />
        <link href="~/css/icons.min.css" rel="stylesheet" />
        <link href="~/css/icons.css" rel="stylesheet" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        <link href="~/css/site.css" rel="stylesheet" />
        <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/morris.js/0.5.1/morris.css">*@
</head>
<body>
    <div class="topTitle">
        <div class="float-right">
            <form class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
                <button type="submit" class="nav-link btn btn-link text-danger">Logout</button>
            </form>
        </div>
    </div>
    <div class="container tread-bg">
        <div class="row my-center">
            <a class="float-left"><img src="~/img/name1.png" class="responsive-img" /></a>
            <div id="navbar">
                <a class="btn" asp-area="" asp-controller="Home" asp-action="Index"><i></i>Infield</a>
                <a class="btn" asp-area="" asp-controller="Clubs" asp-action="Index"><i></i>Clubs</a>
                <a class="btn" asp-area="" asp-controller="Champs" asp-action="Index"><i></i>Championships</a>
                <a class="btn" asp-area="" asp-controller="Events" asp-action="Index"><i></i>Events</a>
                <a class="btn" asp-area="" asp-controller="Races" asp-action="Index"><i></i>Races</a>
                @if (User.IsInRole("Admin"))
                {
                    <a class="btn" asp-area="" asp-controller="UserRoles" asp-action="ManageUserRoles"><i></i>Roles</a>
                }

            </div>
        </div>
        <main role="main" class="pb-3 my-div">
            @RenderBody()
        </main>
        @*<footer class="footer">
                &copy; 2021 - Racesmiths by: Charlie Tincher
            </footer>*@
    </div>


    @*<script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>*@
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/js/vendor.min.js"></script>
    <script src="~/js/vendor.js"></script>
    <script src="~/js/jquery.knob.min.js"></script>
    <script src="~/js/morris.min.js"></script>
    <script src="~/js/raphael.min.js"></script>
    <script src="~/js/dashboard.init.js"></script>
    <script src="~/js/datatables.init.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/morris.js/0.5.1/morris.min.js"></script>
    @*<script src="~/js/app.min.js"></script>*@
    <script src="~/js/app-dark.js"></script>
    <script src="https://cdn.tiny.cloud/1/l7peclxd2nqv523591f18wsp3j66odysd1mpur7xqsih9wm2/tinymce/5/tinymce.min.js" referrerpolicy="origin"></script>


    <script>
        tinymce.init({
            selector: '#mytextarea',
            content_css: 'dark'
        });
    </script>
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.22/js/jquery.dataTables.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
    <script>
        function openNav() {
            document.getElementById("mySidebar").style.width = "250px";
            document.getElementById("mymain").style.marginLeft = "250px";
        }

        function closeNav() {
            document.getElementById("mySidebar").style.width = "0";
            document.getElementById("mymain").style.marginLeft = "0";
        }
    </script>

    <script>
        $(document).ready(function () {
            $('#responsive-datatable').DataTable();
        });
    </script>
  
    <script src="~/js/site.js" asp-append-version="true"></script>
   
    @RenderSection("Scripts", required: false)
</body>
</html>
